  },
  "devDependencies": {
    "@types/faker": "^5.1.5",
    "@types/jest": "^26.0.19",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-sonarjs": "^0.5.0",
    "faker": "^5.1.0",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3",
